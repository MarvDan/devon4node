{
  "name": "@devon4node/cli",
  "version": "1.1.8",
  "description": "devon4node CLI",
  "bin": {
    "devon4node": "./bin/devon4node",
    "d4n": "./bin/devon4node"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:prod": "tsc -p tsconfig.prod.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "npm run build && jasmine src/**/*.spec.js",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devonfw/devon4node"
  },
  "contributors": [
    "Dario Rodriguez Gonzalez <dario.rodriguez-gonzalez@capgemini.com>"
  ],
  "bugs": {
    "url": "https://github.com/devonfw/devon4node/issues"
  },
  "homepage": "https://github.com/devonfw/devon4node",
  "keywords": [
    "devon4node",
    "CLI"
  ],
  "author": {
    "name": "Capgemini",
    "url": "https://www.capgemini.com/"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/node": "^13.7.0",
    "@types/node-fetch": "^2.5.4",
    "@types/yargs": "^15.0.3",
    "prettier": "^1.19.1",
    "tslint": "^6.0.0"
  },
  "dependencies": {
    "@angular-devkit/schematics-cli": "^0.803.24",
    "@devon4node/schematics": "^1.1.7",
    "@nestjs/cli": "^6.14.1",
    "chalk": "^3.0.0",
    "inquirer": "^7.0.4",
    "node-fetch": "^2.6.0",
    "rxjs": "^6.5.4",
    "tmp-promise": "^2.0.2",
    "typeorm": "^0.2.22",
    "yargs": "^15.1.0"
  }
}
